<h2>Test #<%=@test.id %></h2>

<h3>Questions:</h3>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Question text</th>
    </tr>
  </thead>
  <tbody>
    <% @questions.each do |question| %>
      <tr>
        <td><%= question.id %></td>
        <td><%= question.body %></td>
      </tr>
    <% end %>
  </tbody>
</table>