<tr>
  <td><%= test.id %></td>
  <td><%= test.title %></td>
  <td style="text-align: center"><%= test.level %></td>
  <td><%= test.category.title %></td>
  <td style="text-align: center"><%= test.questions.count %></td>
  <td><%= link_to t('.show_link'), admin_test_path(test) %></td>
  <td><%= link_to t('.edit_link'), edit_admin_test_path(test) %></td>
  <td>
    <%= link_to t('.delete_link'),
                admin_test_path(test),
                method: :delete,
                data: { confirm: t('.delete_confirm') }
    %>
  </td>
</tr>