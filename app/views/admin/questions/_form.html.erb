<%= form_with model: [:admin, @test, @question], local: true do |form| %>

  <%= render 'shared/errors', resource: @question %>

  <p class="form-group">
    <%= form.label :body %><br>
    <%= form.text_field :body, id: :question_body, class: 'form-control'%>
  </p>
  <p>
    <%= form.submit class: 'btn btn-secondary'%>
  </p>
<% end %>